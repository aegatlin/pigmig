{
  "name": "pigmig",
  "version": "0.1.0",
  "description": "Pigmig is a postgresql database migration tool.",
  "main": "dist/index.js",
  "bin": {
    "pigmig.newmig": "dist/cli.js"
  },
  "scripts": {
    "build": "rm -rf dist && tsc",
    "pub": "npm version patch && npm publish && git push",
    "test": "mocha",
    "cli": "ts-node src/cli.ts",
    "format": "prettier --check src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aegatlin/pigmig.git"
  },
  "author": "Austin Gatlin",
  "license": "MIT",
  "keywords": [
    "postgres",
    "postgresql",
    "pg",
    "psql",
    "sql",
    "sequel",
    "database",
    "migration",
    "migrator",
    "migrations",
    "small",
    "minimal",
    "typescript"
  ],
  "bugs": {
    "url": "https://github.com/aegatlin/pigmig/issues"
  },
  "homepage": "https://github.com/aegatlin/pigmig#readme",
  "mocha": {
    "require": "ts-node/register",
    "extension": [
      "ts"
    ]
  },
  "dependencies": {
    "eor": "^0.1.5",
    "pg": "^8.5.1"
  },
  "devDependencies": {
    "@types/chai": "^4.2.14",
    "@types/mocha": "^8.0.4",
    "@types/pg": "^7.14.7",
    "chai": "^4.2.0",
    "mocha": "^8.2.1",
    "prettier": "^2.2.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.1.2"
  }
}
